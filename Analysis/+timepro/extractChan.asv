function extractChan(timeline,chanName)
    %%% Will extract specific channel from timeline.
    
    if strcmp(chanName,'time')
        % extract the time channel
        timeline.rawDAQTimestamps;
    else
        syncIndex = find(strcmp({Ttimeline.hw.inputs.name}, chanName));
        if ~isempty(syncIndex)
            tlSync = Timeline.rawDAQData(:,syncIndex);